{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "id": {
      "type": "string"
    },
    "user": {
      "type": "string"
    },
    "product_code": {
      "type": "string"
    },
    "product": {
      "type": "string"
    },
    "review": {
      "type": "string"
    },
    "stars": {
      "type": "integer"
    },
    "created": {
      "type": "string",
      "pattern": "^([1|2][0-9]{3})-((0[1-9])|(1[0-2]))-(([0-2][1-9])|(3[0-1]))T(([0-1][0-9])|(2[0-3])):([0-5][0-9]):([0-5][0-9])Z"
    }
  },
  "required": [
    "id",
    "user",
    "product_code",
    "product",
    "review",
    "stars",
    "created"
  ]
}